<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.MediaPlayer" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->

         <item name="android:statusBarColor">@color/mainColor</item>
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
<!--        <item name="android:windowBackground">@drawable/splash_screen</item>-->

    </style>

    <style name="Theme.MediaPlayer" parent="Base.Theme.MediaPlayer" />

    <!-- ===== For toolbar back arrow color changing ===== -->
    <style name="MyToolbarStyle" parent="Widget.MaterialComponents.Toolbar.Primary">
        <item name="android:tint">@color/white</item>
    </style>

    <!-- ===== For the Shapeable  ImageView =====-->
    <style name="SoftCorner">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">10dp</item>
        <item name="color">@color/white</item>
    </style>
    <!-- ===== SearchViewStyle ===== -->
    <style name="SearchViewStyle" parent="Widget.AppCompat.SearchView">
        <item name="android:keyTextSize">8sp</item>
        <item name="android:background">@drawable/style_search_border</item>
        <item name="queryBackground">@null</item>
    </style>

    <!--  ****************************************  CREATING THEME ************************************************-->

    <style name="MyTextViewStyle">
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@color/white</item>
        <!-- You can add more attributes as needed -->
    </style>



    <attr name="themeColor" format="color"/>

    <!-- ===== Background as well as text color for alert dialog buttons ===== -->
    <attr name="dialogTextColor" format="color"/>
    <attr name="dialogBtnBackground" format="color"/>

